<script type="rocketlazyloadscript" data-rocket-type="text/javascript">window.addEventListener('DOMContentLoaded', function() {
  var apiUrl = 'https://baseapi.elle.vn';
  $('form.newsletter-popup__form').validate({
    rules: {
      email: {
        required: true,
        email: true
      },
      phone: {
        required: false
      },
      name: {
        required: false
      }
    },
    messages: {
      // name: "Vui lòng nhập tên đầy đủ.",
      email: {
        required: "Vui lòng nhập email",
        email: "Địa chỉ email cần theo định dạng name@domain.com"
      },
      // phone: {
      //   required: "Vui lòng nhập số điện thoại."
      // }
    },
    highlight: function(element) {
      $(element).closest('.form-group').addClass('has-error');
    },
    unhighlight: function(element) {
      $(element).closest('.form-group').removeClass('has-error');
    }
  });
  $('#industry_listinng').hide()
  $('#newsletter_submit_form').hide()
  $('#checkbox_form_group').hide()
  $('#newsletter_submit_favorite').hide()
  $('#newsletter-popup-message').hide()
  $('#newsletter_submit_email').click(function() {
    
    var isvalid = $('form.newsletter-popup__form').valid()
    if (isvalid) {
      var testEmail = /^[A-Z0-9._%+-]+@([A-Z0-9-]+\.)+[A-Z]{2,4}$/i;
        if (testEmail.test($('#subscriber_email').val())){
            $('#first_email_input').hide()
            $('#newsletter_submit_email').hide()
            $('#newsletter_submit_favorite').show()
            $('#checkbox_form_group').show()

        }
    
        else  $( "#subscriber_email" ).css({
            background: "yellow",
            border: "1px red solid"
        });
    }else{
      return false
    }
    
  })

  $('#newsletter_submit_favorite').click(function() {
    
    var isvalid = $('form.newsletter-popup__form').valid()
    if (isvalid) {
      $('#newsletter_submit_favorite').hide()
      $('#checkbox_form_group').hide()
      $('#industry_listinng').show()
      $('#newsletter_submit_form').show()
    }else{
      return false
    }
    
  })
  function clearPopup(){
    
    $('#newsletter-popup').removeClass('popup-desk-bakg')
    $('#newsletter-popup').addClass('newsletter-desk-bakg')
  }
  $(".newsletter-popup-btn").click(function(){
    sub_clicked = false
    if ($('#newsletter-popup').hasClass("popup-desk-bakg")) {
      clearPopup()
    }
  });
/**
* Handle newsletter subscriber submit
* data from _getFormData function / this an the form
* return if hash subscriber from getUniqueAudience then run updateAudience else run createNewAudience 
*/
  $('form.newsletter-popup__form').submit(function(e) {
    var isvalid = $(this).valid();
    var url = apiUrl + '/subscribers';
    if (isvalid) {
      var jsonData = _getFormData($(this));
      if (jsonData) {
        getUniqueAudience(url, jsonData); 
      }
    }
    return false;
  });
  /**
   * Author: Ngan Nguyen
   * Function createNewAudience / for create new user subscribe add new record in baseapi.elle.vn microsite and mailchimp api
   * Param string api / an define api url
   * Param json subscriber / an user Object 
   * return message
   */
  $('#reg_js_img').hide();
  $.ajaxSetup({
    beforeSend: function(){
        $("#newsletter_submit_form").hide();
        $('#reg_js_img').show();
    },
    error: function(xhr, status, error) {
        $('#reg_js_img').hide();
        $('#industry_listinng').hide()
        $('#first_email_input').show()
        $('#newsletter_submit_form').show()

        $('#subscriber_email').css('border', 'solid 2px red')
        swal(xhr.responseText)
    }
  });
  function createNewAudience(api, subscriber) {
    //TODO configable.
    var message = '';
    $('#newsletter-popup-message').html(message);
    $.post(api, subscriber).done(function( data ) {
      if (data.error == undefined) {
        message = 'Chúc mừng bạn đã đăng ký nhận bản tin thành công!';
        $('#newsletter-popup-message').html(message);
        $('#newsletter-popup-message').show();
        $('form.newsletter-popup__form').html('');
      }
      else {
        message = '<span style="color:red;font-size:12px;margin-bottom:5px;">Vui lòng kiểm tra lại kết nối internet của bạn.</span>';
        $('#newsletter-popup-message').html(message);
        return false
      }
    }); 
  }
  /**
  * Author: Ngan Nguyen
  * Function getUniqueAudience / for if get user subscribe with unique email
  * Param string api 
  * Param json subscriber
  * return json subscriber plus subscriber ID
  */
  function getUniqueAudience(api, subscriber) {
    $.get(api+"?email="+subscriber.email).done(function( data ){
      if (data.error==undefined) {
        if (data.length === 0) {
          subscriber.elle = true
          subscriber.elleman = false
          subscriber.elledecoration = false
          createNewAudience(api, subscriber)
        } else {
          subscriber.elle = true
          subscriber.elleman = data[0].elleman
          subscriber.elledecoration = data[0].elledecoration 
          updateAudience(api, data[0]._id, subscriber) 
        }
      }else{
        return false
      }
    })
  }
  /**
  * Author: Ngan Nguyen
  * Function updateAudience / for update user subscribe
  * Param string api
  * Param id
  * Param json subscriber
  * return message
  */

  function updateAudience(api, id, subscriber) {
    //TODO configable.
    
    var message = '';
    $('#newsletter-popup-message').html(message);
    $.ajax({
      url: api+"/"+id,
      type: 'PUT',
      data: subscriber, 
      success: function(data) {

        if (data.error == undefined) {
          message = 'Chúc mừng bạn đã cập nhật bản tin thành công!';
          $('#newsletter-popup-message').html(message);
          $('#newsletter-popup-message').show();
          $('form.newsletter-popup__form').html('');
        }
        else {
          message = '<span style="color:red;font-size:12px;margin-bottom:5px;">Vui lòng kiểm tra lại kết nối internet của bạn. </span>';
          $('#newsletter-popup-message').html(message);
          return false
        }
      }
    });

  }
  function _getFormData($form){
    var unindexed_array = $form.serializeArray();
    var indexed_array = {
      'fashion' : false,
      'all_in_one': false,
      'beauty': false,
      'entertainment': false,
      'horoscope': false,
      'status': 'subscribed'
    };
    let industry_arr = []
    $.map(unindexed_array, function(n, i){
      if (n['value'] == 'on') {
        n['value'] = true;
      }
      
      if (n['name'] === 'industry_newsletter[]') {
        industry_arr.push(n['value'])
        // unindexed_array.splice(i, 1) 
      }else{
        indexed_array[n['name']] = n['value'];
      }
      
    });
    indexed_array['industry'] = industry_arr.join();
    if (indexed_array['industry']) {
      return indexed_array; 
    }else{
      swal("Vui lòng chọn lĩnh vực !")
      return false
    }
    
  }


  $( document ).ready(function() {
    var hash = location.hash.replace("'", '');
    const hashtags=location.hash.match(new RegExp('#([^&]*)'));
    
    var params = {};
    
    hash.split('&').map(hk => {
      let temp = hk.split('=');
      var key  = filterXSS(temp[0]);
      var value  = filterXSS(temp[1]);
      params[key] = value;
    });
    if (params['subscriber'] && params['subscriber'].length > 0 ) {
      var url = apiUrl + 'subscribe';
      $.get(url, {"hash": params['subscriber']}, ).done(function( data ) {
        $('.newsletter-popup__form input[name*="email"]').val(data.email);
        $('.newsletter-popup__form input[name*="phone"]').val(data.phone);
        $('.newsletter-popup__form input[name*="name"]').val(data.name);
        
        $('.newsletter-popup__form input[name*="fashion"]').prop('checked', data.fashion);
        $('.newsletter-popup__form input[name*="beauty"]').prop('checked', data.beauty);
        $('.newsletter-popup__form input[name*="entertainment"]').prop('checked', data.entertainment);
        $('.newsletter-popup__form input[name*="horoscope"]').prop('checked', data.horoscope);
        $('.newsletter-popup__form input[name*="all_in_one"]').prop('checked', data.all_in_one);
        var string = '<input type="hidden" name="id" value="' + params['subscriber'] + '" />';
        $(string).appendTo('.newsletter-popup__form');
      });
    }
    
    if (hashtags != null && hashtags.length > 0 && hashtags[1] == 'newsletter-popup') {
      $.magnificPopup.open({
        items: { src: '#' + hashtags[1] },
        type: 'inline'
      });
    }

  });
  
});</script>
